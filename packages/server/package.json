{
  "name": "mutaserv",
  "version": "0.0.5",
  "author": "Patrick Borowy <patrick@krzepah.net>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development node package/index",
    "prestart": "npm run create-sqlite-db",
    "test": "npm run create-sqlite-db; cross-env SECRET=TEST LOGS_PATH=./test/tests.log REDUCERS=`pwd`/test/reducers.js jest --coverage; npm run drop-sqlite-db ; npm run coverage",
    "create-sqlite-db": "shx touch ./database.sqlite",
    "drop-sqlite-db": "shx rm ./database.sqlite",
    "coverage": "coverage-badger -r coverage/clover.xml -d coverage"
  },
  "eslintConfig": {
    "extends": "eslint-config-developit",
    "rules": {
      "prefer-rest-params": 0
    }
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "dependencies": {
    "@babel/generator": "^7.5.5",
    "@babel/parser": "^7.5.5",
    "@parcel/watcher": "^2.0.0-alpha.3",
    "@polka/send-type": "^0.5.2",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "polka": "latest",
    "ramda": "^0.26.1",
    "require-from-string": "^2.0.2",
    "sade": "^1.6.0",
    "sequelize": "^5.10.3",
    "shx": "^0.3.2",
    "sirv": "^0.4.2",
    "sqlite3": "^4.0.9",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "coverage-badger": "^1.0.1",
    "eslint": "^6.1.0",
    "eslint-config-developit": "^1.1.1",
    "jest": "^24.8.0",
    "pre-commit": "^1.2.2",
    "supertest": "^4.0.2"
  },
  "bin": {
    "mutaserv": "./package/command.js"
  }
}
